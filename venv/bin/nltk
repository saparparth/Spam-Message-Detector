#!/workspaces/Spam-Message-Detector/venv/bin/python
# -*- coding: utf-8 -*-
import re
import sys
from nltk.cli import cli
import nltk
nltk.download('punkt')
nltk.download('wordnet')
nltk.download('omw-1.4')
if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw|\.exe)?$', '', sys.argv[0])
    sys.exit(cli())
